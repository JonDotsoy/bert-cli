{
  "name": "bert.js",
  "version": "0.0.11",
  "description": "A task manager inspired on Gulp.",
  "main": "index.js",
  "engines": {
    "node": ">= 7.6.0"
  },
  "scripts": {
    "postpublish": "sh -c \"git tag v$npm_package_version\"",
    "gulp": "node --harmony-async-await node_modules/gulp/bin/gulp.js --gulpfile .gulp.js",
    "env": "env",
    "standard": "standard --fix",
    "nsp": "nsp",
    "test": "mocha --harmony-async-await",
    "testw": "mocha --harmony-async-await -w"
  },
  "bin": {
    "bert": "bin/bert.js"
  },
  "keywords": [],
  "author": "Jonathan Delgado <hi@jon.soy>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "dotenv": "^4.0.0",
    "gulp": "^3.9.1",
    "mocha": "^3.2.0",
    "nsp": "^2.6.3",
    "standard": "^9.0.2"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "debug": "^2.6.2",
    "lodash": "^4.17.4",
    "minimist": "^1.2.0",
    "moment": "^2.17.1",
    "orchestrator": "^0.3.8",
    "resolve": "^1.3.2",
    "util": "^0.10.3"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JonDotsoy/bert-cli.git"
  },
  "bugs": {
    "url": "https://github.com/JonDotsoy/bert-cli/issues"
  },
  "homepage": "https://jondotsoy.github.io/bert/"
}
